IF OBJECT_ID('original.listings', 'U') IS NOT NULL
    DROP TABLE original.listings;

CREATE TABLE original.listings (
    id INT,
    listing_url NVARCHAR(MAX),
    scrape_id BIGINT,
    last_scraped DATE,
    source NVARCHAR(255),
    name NVARCHAR(MAX),
    description NVARCHAR(MAX),
    neighborhood_overview NVARCHAR(MAX),
    picture_url NVARCHAR(MAX),
    host_id INT,
    host_url NVARCHAR(MAX),
    host_name NVARCHAR(255),
    host_since DATE,
    host_location NVARCHAR(255),
    host_about NVARCHAR(MAX),
    host_response_time NVARCHAR(255),
    host_response_rate NVARCHAR(50),
    host_acceptance_rate NVARCHAR(50),
    host_is_superhost NVARCHAR(10),
    host_thumbnail_url NVARCHAR(MAX),
    host_picture_url NVARCHAR(MAX),
    host_neighbourhood NVARCHAR(255),
    host_listings_count INT,
    host_total_listings_count INT,
    host_verifications NVARCHAR(MAX),
    host_has_profile_pic NVARCHAR(10),
    host_identity_verified NVARCHAR(10),
    neighbourhood NVARCHAR(255),
    neighbourhood_cleansed NVARCHAR(255),
    latitude FLOAT,
    longitude FLOAT,
    property_type NVARCHAR(255),
    room_type NVARCHAR(255),
    accommodates INT,
    bathrooms FLOAT,
    bathrooms_text NVARCHAR(255),
    bedrooms FLOAT,
    beds FLOAT,
    amenities NVARCHAR(MAX),
    price NVARCHAR(50),
    minimum_nights INT,
    maximum_nights INT,
    minimum_minimum_nights INT,
    maximum_minimum_nights INT,
    minimum_maximum_nights INT,
    maximum_maximum_nights INT,
    minimum_nights_avg_ntm FLOAT,
    maximum_nights_avg_ntm FLOAT,
    calendar_updated NVARCHAR(255),
    has_availability NVARCHAR(10),
    availability_30 INT,
    availability_60 INT,
    availability_90 INT,
    availability_365 INT,
    calendar_last_scraped DATE,
    number_of_reviews INT,
    number_of_reviews_ltm INT,
    number_of_reviews_l30d INT,
    first_review DATE,
    last_review DATE,
    review_scores_rating FLOAT,
    review_scores_accuracy FLOAT,
    review_scores_cleanliness FLOAT,
    review_scores_checkin FLOAT,
    review_scores_communication FLOAT,
    review_scores_location FLOAT,
    review_scores_value FLOAT,
    license NVARCHAR(100),
    instant_bookable NVARCHAR(10),
    calculated_host_listings_count INT,
    calculated_host_listings_count_entire_homes INT,
    calculated_host_listings_count_private_rooms INT,
    calculated_host_listings_count_shared_rooms INT,
    reviews_per_month FLOAT
);
